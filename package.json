{
  "name": "omni-platform",
  "version": "3.0.0",
  "description": "Enterprise multi-tenant platform with modular architecture",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:complete": "tsx prisma/seed-complete.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "workers": "tsx src/workers/index.ts",
    "mcp:dev": "tsx src/mcp/server.ts",
    "mcp:build": "tsc src/mcp/server.ts --outDir dist/mcp",
    "mcp:start": "node dist/mcp/server.js",
    "web:dev": "cd web && npm run dev",
    "web:build": "cd web && npm run build",
    "web:preview": "cd web && npm run preview",
    "web:install": "cd web && npm install"
  },
  "keywords": [
    "enterprise",
    "multi-tenant",
    "crm",
    "erp",
    "knowledge-graph",
    "ai",
    "omnichannel"
  ],
  "author": "OMNI Platform Team",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/storage": "^7.18.0",
    "@hubspot/api-client": "^13.4.0",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@prisma/client": "^5.22.0",
    "@qdrant/js-client-rest": "^1.11.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.21.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "js-cookie": "^3.0.5",
    "jsforce": "^3.10.10",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
    "minio": "^8.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.16",
    "openai": "^4.73.0",
    "pdf-parse": "^2.4.5",
    "pdfkit": "^0.17.2",
    "pino": "^9.5.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^13.0.0",
    "prom-client": "^15.1.3",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "stripe": "^20.1.0",
    "twilio": "^5.11.1",
    "web-push": "^3.6.7",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/luxon": "^3.7.1",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.19.3",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/pdfkit": "^0.17.4",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.14.0",
    "jest": "^29.7.0",
    "prisma": "^5.22.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
